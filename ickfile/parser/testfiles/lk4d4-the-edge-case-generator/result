(from "busybox:buildroot-2014.02")
(label "maintainer" "docker <docker@docker.io>")
(onbuild (please "echo" "test"))
(onbuild (please "echo test"))
(onbuild (copy "." "/"))
(please "ls" "-la")
(please "echo" "'1234'")
(please "echo \"1234\"")
(please "echo 1234")
(please "echo '1234' &&     echo \"456\" &&     echo 789")
(please "sh -c 'echo root:testpass         > /tmp/passwd'")
(please "mkdir -p /test /test2 /test3/test")
(env "SCUBA" "1 DUBA 3")
(env "SCUBA" "\"1 DUBA 3\"")
(cmd "echo" "test")
(cmd "echo test")
(cmd "echo \"test\"")
(cmd "echo 'test'")
(cmd "echo 'test' | wc -")
(expose "3000")
(expose "9000" "5000" "6000")
(user "docker")
(user "docker:root")
(volume "/test")
(volume "/test" "/test2")
(volume "/test3")
(workdir "/test")
(add "." "/")
(copy "." "copy")
